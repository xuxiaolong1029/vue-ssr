{
  "name": "vue-ssr",
  "version": "1.0.0",
  "description": "关于对vue老项目的服务端渲染的改造demo",
  "author": "xuxiaolong",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "pre": "cross-env NODE_ENV=prepare webpack-dev-server --hot",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --hot",
    "start": "cross-env NODE_ENV=production nodemon server/server.js",
    "server": "cross-env NODE_ENV=production node server.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules"
  },
  "dependencies": {
    "element-ui": "^2.13.0",
    "koa": "^2.11.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "autoprefixer": "^9.7.2",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "koa-favicon": "^2.0.1",
    "koa2-connect": "^1.0.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "nprogress": "^0.2.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "postcss": "^7.0.36",
    "postcss-loader": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
